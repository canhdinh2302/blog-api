{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blogs"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 250
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "cover": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "categories": {
      "collection": "categories",
      "via": "blogs",
      "dominant": true
    },
    "tags": {
      "via": "blogs",
      "collection": "tags"
    }
  }
}
