{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "name": "todos"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "permission",
      "via": "todos"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "monthly",
        "weekly",
        "daily",
        "one_time"
      ],
      "default": "one_time"
    },
    "time": {
      "type": "time"
    },
    "todo_logs": {
      "via": "todo",
      "collection": "todo-logs"
    }
  }
}
